<#@ template language="C#" #>
<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <!--复制本地锁定的程序集到输出目录-->
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.App" />
    <PackageReference Include="<#= _prefix #>.Lib.Cache.MemoryCache" Version="1.0.0" />
    <PackageReference Include="<#= _prefix #>.Lib.Cache.Redis" Version="1.0.0" />
    <PackageReference Include="<#= _prefix #>.Lib.Data.SqlServer" Version="1.0.0" />
    <PackageReference Include="<#= _prefix #>.Lib.Data.MySql" Version="1.0.0" />
    <PackageReference Include="<#= _prefix #>.Lib.Data.SQLite" Version="1.0.0" />
    <PackageReference Include="<#= _prefix #>.Lib.Host.Web" Version="1.0.0" />
    <PackageReference Include="<#= _prefix #>.Module.Admin.Web" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Web\Web.csproj" />
  </ItemGroup>

  <!--复制配置文件到输出目录-->
  <ItemGroup>
    <Content Update="config\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
